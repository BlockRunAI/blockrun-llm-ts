{
  "name": "@blockrun/llm",
  "version": "0.1.0",
  "description": "BlockRun LLM Gateway SDK - Pay-per-request AI via x402 on Base",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "llm",
    "ai",
    "x402",
    "base",
    "usdc",
    "micropayments",
    "openai",
    "claude",
    "gemini",
    "blockchain"
  ],
  "author": "BlockRun <hello@blockrun.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blockrun/blockrun-llm-ts"
  },
  "homepage": "https://blockrun.ai",
  "bugs": {
    "url": "https://github.com/blockrun/blockrun-llm-ts/issues"
  },
  "dependencies": {
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
